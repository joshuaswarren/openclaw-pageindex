{
  "name": "openclaw-pageindex",
  "version": "0.1.0",
  "description": "OpenClaw PageIndex - vectorless, reasoning-based document search using OpenClaw's LLM providers. Native TypeScript implementation of PageIndex's hierarchical document tree traversal.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "document-search",
    "rag",
    "llm",
    "vectorless",
    "reasoning",
    "hierarchical",
    "pdf",
    "markdown",
    "typescript"
  ],
  "author": "Joshua Warren",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joshuaswarren/openclaw-pageindex.git"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/pdf-parse": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "tsup": "^8.5.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
